<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.iscae._election_etudient.Login.data.User">
        <table name="_user" schema="election_etudient_db" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="email">
                <column name="email" length="20"/>
            </basic>
            <basic name="nom">
                <column name="nom" length="20"/>
            </basic>
            <basic name="password">
                <column name="password" length="256"/>
            </basic>
            <basic name="role">
                <column name="role" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
